    "from time import sleep\n",
    "\n",
    "def sleep_decorator(function):\n",
    "\n",
    "    def wrapper():\n",
    "        sleep(5)\n",
    "        function()\n",
    "        \n",
    "    return wrapper\n",
    "\n",
    "\n",
    "@sleep_decorator\n",
    "def print_number():\n",
    "    print(333)\n",
    "    \n",
    "print_number()"
   ]
