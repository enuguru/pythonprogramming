   "source": [
    "# metaclass concept"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "a85b4f4f",
   "metadata": {},
   "outputs": [],
   "source": [
    "class MyMeta(type):\n",
    "    pass\n",
    "\n",
    "class MyClass(metaclass=MyMeta):\n",
    "    pass\n",
    "\n",
    "class MySubclass(MyClass):\n",
    "    pass"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "67a7ce59",
   "metadata": {},
