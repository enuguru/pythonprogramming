   "source": [
    "# Python program to show infinite iterators\n",
    "\n",
    "import itertools\n",
    "\n",
    "l = ['Geeks', 'for', 'Geeks']\n",
    "\n",
    "# defining iterator\n",
    "iterators = itertools.cycle(l)\n",
    "\n",
    "# for in loop\n",
    "for i in range(6):\n",
    "    print(next(iterators), end = \" \")"
   ]
