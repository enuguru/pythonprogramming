   "source": [
    "def count_words(list1):\n",
    "  count = {}\n",
    "  for word in list1:\n",
    "    if word in count:\n",
    "      count[word] += 1\n",
    "    else:\n",
    "      count[word] = 1\n",
    "  return count\n",
    "\n",
    "# Driver code\n",
    "list1 = ['red', 'green', 'blue', 'white', 'black', 'orange', 'red']\n",
    "print(count_words(list1))"
   ]
  }
