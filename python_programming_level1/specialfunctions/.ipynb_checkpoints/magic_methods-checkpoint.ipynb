   "source": [
    "class MyList(list):\n",
    "    def __contains__(self, typ):\n",
    "        for val in self:\n",
    "            if isinstance(val, typ):\n",
    "                return True\n",
    "        return False\n",
    "\n",
    "x = MyList([1, 2, 'a', 3])\n",
    "\n",
    "print(float in x) # False\n",
    "print(str in x)   # True"
   ]
  }
